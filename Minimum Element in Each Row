#include <stdio.h>
#include <limits.h>

int main() {
    int rows, cols;
    
    if (scanf("%d %d", &rows, &cols) != 2) return 0;

    for (int i = 0; i < rows; i++) {
        int min_val = INT_MAX; 
        
        for (int j = 0; j < cols; j++) {
            int current;
            scanf("%d", &current);
            
            if (current < min_val) {
                min_val = current;
            }
        }
        
        printf("%d\n", min_val);
    }

    return 0;
}
